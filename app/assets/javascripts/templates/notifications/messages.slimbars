ul
  | {{#each messages}}
  li
    a href='{{url}}'
      .media
        .pull-left
          img.media-object src='{{message.sender.image}}' width='50' height='50' alt=''
        .media-body
          strong.text-truncate
            | {{message.sender.name}}
          .text-muted
            span.text-truncate
              | {{message.body}}
            small
              | {{localize "datetime.formats.message" message.date_sent}}
  | {{/each}}
